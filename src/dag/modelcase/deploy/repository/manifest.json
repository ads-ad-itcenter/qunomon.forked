{
    "meta": {
        "name": "eval_mnist_acc_test_by_tfmodel_1.0",
        // AITのバージョン
        "verson": 1,
        // measurement_idをquality_urlへ変更
        "quality_url": "https://quality.go.jp/正確性",
        "description": "* データ入力形式: XXX\n* MLエンジン: tensorflow-vX.X\n* 出力: XXX\n"
    },

    "input": {
        // テスト実行時に用いるパラメータ
        // name: パラメータの名前(AIT作成者は好きな名前をつけることができる)
        // type: Pythonにおける型
        // description: パラメータの説明。フロントエンドで表示される予定。(省略可)
        // required: テスト実行に必ず入力を要求するかどうか。
        // default_val: 値の入力がされていないときのデフォルト値(省略可)
        "parameters": [
            {
                "name": "epsilon",
                "type": "float",
                "description": "敵対的生成のずらし具合, \\epsilon \\in \\{0.0, 1.0\\}",
                "required": true
            },
            {
                "name": "delta",
                "type": "int",
                "description": "",
                "required": false
            },
            {
                "name": "msg",
                "type": "str",
                "description": "メッセージとか",
                "required": false,
                "default_val": "default"
            }
        ],
        // インベントリ
        // name: インベントリの名前(AIT作成者は好きな名前をつけることができる)
        // extensions: インベントリに格納されるファイルの拡張子
        // tag: インベントリのファイルの特徴を記録する(使いたいインベントリを検索するときに使う)
        // description: インベントリの説明。フロントエンドで表示される予定。(省略可)
        "inventories": [
            {
                "name": "model_inv_aaa",
                "extension": "h5",
                "tag" : [
                    "AI_MODEL",
                    "TF_MODEL"
                ],
                "description": "Tensorflowで作成したモデル"
            },
            {
                "name": "dataset_inv_bbb",
                "extension": "png",
                "tag" : [
                    "image"
                ],
                "description": "1枚の手書き文字データ(28x28, png)のpng"
            },
            {
                "name": "dataset_inv_ccc",
                "extension": "zip",
                "tag" : [
                    "archive",
                    "image"
                ],
                "description": "複数の手書き文字データ(28x28, png)のzip"
            }
        ]
    },

    "output":{
        // measures: テスト結果の数値を指す。この値を用いてテストの合否を決定する。
        // measureに何も書かなかった場合、
        // 省略(すべて書かなかった場合、自動成功)
        "measures": [
            {
                "name": "mae",
                "type": "value",
                "description": "mean absolute error"
            },
            {
                "name": "mse",
                "type": "value",
                "description": "mean squared error"
            },
            {
                "name": "coverage",
                "type": "range",
                "description": "なんらかのカバー範囲"
            }
        ],
        // テスト実行により生成されたファイルの内、
        // レポート表示の際に用いられる、メディアファイルはここに書く
        "report_resources":[
            {
                "name": "distribution_graph",
                "path": "aa/bb/cc/distribution_graph.png",
                "type": "image",
                "description": "分布"
            },
            {
                "name": "scatter_graph",
                "path": "aa/bb/cc/scatter_graph.png",
                "type": "image",
                "description": "散布図"
            }
        ],
        // テスト実行により生成されたファイルの内、
        // フロントエンドにてダウンロード可能とするファイルはここに書く
        // 注1: report_resourcesに書かれたもののと同じものを書くことも可能
        //      その場合、指定されたファイルは、「レポートで用いられる」且つ「ダウンロード可」となる。
        // 注2: 注1とは逆に、report_resourcesには書かなかったファイルをdownloadsに書くと、
        //      「レポートに使われない」が「ダウンロードは可能」となる。
        "downloads": [
            {
                "name": "adversarial_sample",
                "path": "/aa/bb/cc/image.png",
                "description": "one image"
            },
            {
                "name": "adversarial_samples",
                "path": "/aa/bb/cc",
                "description": "zipped images"
            }
        ]
    }

}