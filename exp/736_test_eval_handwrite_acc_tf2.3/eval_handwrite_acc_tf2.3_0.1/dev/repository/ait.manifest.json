{
  "name": "eval_handwrite_acc_tf2.3",
  "description": "Only Sequential API Model\n\n* QualityMeasurement\nAccuracy=TP+TNTP+FP+FN+TN\nPrecision=TPTP+FP\nRecall=TPTP+FN\nF−measure=2Recall∗PrecisionRecall+Precision\nMicro−average=∑Ni=0xi∑Ni=0ni\nMacro−average=1N∑Ni=0xini\nAUC\n\n*Resources\nROC曲線\n混同行列",
  "author": "AIST",
  "email": "",
  "version": "0.1",
  "quality": "https://airc.aist.go.jp/aiqm/quality/internal/学習モデルの正確性",
  "reference": "",
  "inventories": [
    {
      "name": "trained_model",
      "type": "model",
      "description": "Tensorflow 2.3で学習したモデル",
      "format": [
        "h5"
      ],
      "schema": "https://support.hdfgroup.org/HDF5/doc/"
    },
    {
      "name": "test_set_images",
      "type": "dataset",
      "description": "テスト画像セット（MNISTフォーマット）",
      "format": [
        "gz"
      ],
      "schema": "http://yann.lecun.com/exdb/mnist/"
    },
    {
      "name": "test_set_labels",
      "type": "dataset",
      "description": "テスト画像ラベル（MNISTフォーマット）",
      "format": [
        "gz"
      ],
      "schema": "http://yann.lecun.com/exdb/mnist/"
    }
  ],
  "parameters": [
    {
      "name": "class_count",
      "type": "int",
      "description": "multiple classification class number",
      "default_val": "10"
    },
    {
      "name": "image_px_size",
      "type": "int",
      "description": "prediction image pixel size",
      "default_val": "28"
    },
    {
      "name": "auc_average",
      "type": "string",
      "description": "{‘micro’, ‘macro’, ‘samples’, ‘weighted’}\r\nref:\r\nhttps://scikit-learn.org/stable/modules/generated/sklearn.metrics.roc_auc_score.html",
      "default_val": "macro"
    },
    {
      "name": "auc_multi_class",
      "type": "string",
      "description": "{‘raise’, ‘ovr’, ‘ovo’}\nref:\nhttps://scikit-learn.org/stable/modules/generated/sklearn.metrics.roc_auc_score.html",
      "default_val": "raise"
    }
  ],
  "report": {
    "measures": [
      {
        "name": "Accuracy",
        "type": "float",
        "description": "(TP+TN) / (TP+FP+FN+TN)",
        "structure": "single"
      },
      {
        "name": "Precision",
        "type": "float",
        "description": "TP / (TP+FP)",
        "structure": "single"
      },
      {
        "name": "Recall",
        "type": "float",
        "description": "TP / (TP+FN)",
        "structure": "single"
      },
      {
        "name": "F−measure",
        "type": "float",
        "description": "(2Recall∗Precision) / (Recall+Precision)",
        "structure": "single"
      },
      {
        "name": "AUC",
        "type": "float",
        "description": "Area under the ROC curve",
        "structure": "single"
      },
      {
        "name": "AccuracyByClass",
        "type": "float",
        "description": "Accuracy for each class.",
        "structure": "sequence"
      },
      {
        "name": "PrecisionByClass",
        "type": "float",
        "description": "Precision for each class.",
        "structure": "sequence"
      },
      {
        "name": "RecallByClass",
        "type": "float",
        "description": "Recall for each class.",
        "structure": "sequence"
      },
      {
        "name": "F−measureByClass",
        "type": "float",
        "description": "F−measure for each class.",
        "structure": "sequence"
      }
    ],
    "resources": [
      {
        "name": "ConfusionMatrixCSV",
        "path": "/usr/local/qai/resources/1/confusion_matrix.csv",
        "type": "picture",
        "description": "混同行列"
      },
      {
        "name": "ConfusionMatrixHeatmap",
        "path": "/usr/local/qai/resources/2/confusion_matrix.png",
        "type": "picture",
        "description": "混同行列(ヒートマップ)"
      },
      {
        "name": "ROC-curve",
        "path": "/usr/local/qai/resources/3/roc_curve.png",
        "type": "picture",
        "description": "ROC曲線"
      },
      {
        "name": "NGPredictImages",
        "path": "/usr/local/qai/resources/4/ng_predict_actual_class_{}.png",
        "type": "picture",
        "description": "推論NGとなった画像の一覧を、正解ラベルの枚数分だけ出力する"
      }
    ]
  },
  "downloads": [
    {
      "name": "Log",
      "path": "/usr/local/qai/downloads/1/ait.log",
      "description": "AIT実行ログ"
    },
    {
      "name": "PredictionResult",
      "path": "/usr/local/qai/downloads/2/prediction.csv",
      "description": "ID,正解ラベル,推論結果確率(ラベル毎)"
    }
  ]
}